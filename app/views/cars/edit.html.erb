<h1>Editing Car</h1>

<%= form_for(@car) do |form| %>

  <div>
    <%= form.label :make %>
    <%= form.text_field :make %>
  </div>

  <div>
    <%= form.label :model %>
    <%= form.text_field :model %>
  </div>

  <div>
    <%= form.label :year %>
    <%= form.text_field :year %>
  </div>

  <div>
    <%= form.label :price %>
    <%= form.text_field :price %>
  </div>

  <%= form.submit %>
<% end %>

